<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        targetNamespace="https://github.com/akeusen/BFH_Projects/blob/master/src/ch/bfh/ti/lottery/tickets/ticket"
        xmlns:ticketType="https://github.com/akeusen/BFH_Projects/blob/master/src/ch/bfh/ti/lottery/tickets/ticketType"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        elementFormDefault="qualified"
        >

    <xs:import
            namespace="https://github.com/akeusen/BFH_Projects/blob/master/src/ch/bfh/ti/lottery/tickets/ticketType"
            schemaLocation="ticketType.xsd"/>

    <xs:element name="ticket">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ticketType:ticketType">
                    <xs:attribute name="ticketId" type="xs:int" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

</xs:schema>
